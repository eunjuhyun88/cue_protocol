{
  "name": "ai-personal-backend",
  "version": "1.0.0",
  "description": "AI Personal Assistant Backend with DI Container",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "kill-port": "lsof -ti:3001 | xargs kill -9 || echo \"Port 3001 is not in use\"",
    "dev-fresh": "npm run kill-port && sleep 1 && npm run dev",
    "lint": "eslint src --ext .ts --fix",
    "test": "echo 'Tests not implemented yet'",
    "dev-clean": "npm run kill-port && sleep 2 && npm run dev",
    "dev-ws": "npm run kill-port && sleep 2 && tsx watch src/app.ts",
    "start-ws": "npm run kill-port && sleep 2 && node dist/app.js"
  },
  "keywords": [
    "ai",
    "passport",
    "webauthn",
    "cue-tokens",
    "typescript",
    "express"
  ],
  "author": "AI Personal Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.9.1",
    "@google/generative-ai": "^0.24.1",
    "@simplewebauthn/server": "^9.0.3",
    "@supabase/supabase-js": "^2.50.2",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^4.104.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}